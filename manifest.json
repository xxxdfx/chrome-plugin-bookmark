{
  "manifest_version": 3,
  "name": "书签快捷访问",
  "version": "1.0",
  "description": "快速访问您的书签",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": ["bookmarks", "storage"]
}